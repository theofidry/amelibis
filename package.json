    "errorhandler": "^1.1.1",
    "loopback": "^2.8.0",
    "loopback-boot": "^2.4.0",
    "loopback-connector-mongodb": "^1.8.0",
    "loopback-datasource-juggler": "^2.7.0",
    "mime": "^1.3.4",
    "multer": "^0.1.7",
    "node-dbf": "^0.1.0",
    "serve-favicon": "^2.0.1"
  },
  "optionalDependencies": {
    "loopback-explorer": "^1.1.0"
  },
  "devDependencies": {
    "grunt": "^0.4.5",
    "grunt-loopback-angular": "^1.1.0",
    "grunt-loopback-sdk-angular": "^1.1.2",
    "jshint": "^2.5.6",
    "mocha": "^2.1.0",
    "node-mocks-http": "^1.2.4",
    "unit.js": "^1.1.3"
  },
  "repository": {
